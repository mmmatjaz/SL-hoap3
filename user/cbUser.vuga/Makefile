headers:: header
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for d in $(SUBDIRS) ;\
	do \
		if [ -f $$d/Makefile ]; then \
		(cd $$d ; $(MAKE) $(MFLAGS) headers) ; \
		fi; \
	done

